    Layout = "~/Views/Shared/_LayoutAfterLogin.cshtml";
    ViewBag.Title = "Select Line & Model";
}

<style>
    html, body {
        height: 100%;
    }

    body {
        background: #ffffff;
    }
</style>


<div class="l-box__select-model-line ">
    <button type="button" class="btn-back-homes-corner " data-dismiss="modal" onclick="window.location='@Url.Action("MainMenu", "FrontEnd")'"><i class="fas fa-home"></i> </button>

    <div class="h-text--title-box-form-v1">Please select Line & Model</div>

    <div class="solid-line-width"></div>

    <table class="table table-condensed table-borderless margin-t-30 margin-b-30">
        <tr>
            <td width="30%" class="h-text__point-jobs-name"><i class="fas fa-dot-circle fa-fw color-cyan-green"></i> Plant</td>
            <td class="h-text__point-jobs-desc">:  2300</td>
        </tr>

        <tr>
            <td width="30%" class="h-text__point-jobs-name"><i class="fas fa-dot-circle fa-fw color-cyan-green"></i> Dept</td>
            <td class="h-text__point-jobs-desc">:  DH</td>
        </tr>

        <tr>
            <td width="30%" class="h-text__point-jobs-name"><i class="fas fa-dot-circle fa-fw color-cyan-green"></i> Leader</td>
            <td class="h-text__point-jobs-desc">:  Iskandar</td>
        </tr>
    </table>

    <select class="form-control c-form__select-option margin-b-30">
        <option>DH-01</option>
        <option>DH-02</option>
        <option>DH-03</option>
        <option>DH-04</option>
    </select>

    <select class="form-control c-form__select-option margin-b-30">
        <option>DH C2000 2N</option>
        <option>DH C3000 3N</option>
        <option>DH C4000 4N</option>
        <option>DH C5000 5N</option>
    </select>

    <label class="c-login__container-checkbox">
        One By One
        @Html.CheckBox("isNewlyEnrolled")
        <span class="c-login__checkmark"></span>
    </label>

    <input type="submit" class="btn-oranges" value="APPLY" onclick="window.location='@Url.Action("ListProcessChecking", "FrontEnd")'" />
</div>











